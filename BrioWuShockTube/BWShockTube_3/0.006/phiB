FoamFile
{
    format      ascii;
    class       surfaceScalarField;
    location    "0.006";
    object      phiB;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -2 0 0 -1 0];

internalField   uniform 3;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform -3;
    }
    outlet
    {
        type            calculated;
        value           uniform 3;
    }
    lowerWall
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
    upperWall
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
}


