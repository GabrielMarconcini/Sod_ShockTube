FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    (-1 -1 -1)	//ponto 1
    (1 -1 -1)	//ponto 2
    (1 1 -1)	//ponto 3
    (-1 1 -1)	//ponto 4
    (-1 -1 1)	//ponto 5
    (1 -1 1)	//ponto 6
    (1 1 1)	//ponto 7
    (-1 1 1)	//ponto 8
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (100 1 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    inlet
    {
        type wall;
        faces
        (
            (0 4 7 3)
        );
    }
    outlet
    {
        type wall;
        faces
        (
            (2 6 5 1)
        );
    }
    lowerWall
    {
        type empty;
        faces
        (
            (1 5 4 0)
        );
    }
    upperWall
    {
        type empty;
        faces
        (
            (3 7 6 2)
        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (0 3 2 1)
            (4 5 6 7)
        );
    }
);

mergePatchPairs
(
);

